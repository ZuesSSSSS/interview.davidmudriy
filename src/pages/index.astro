---
import ProjectLayout from  '../components/ProjectLayout.astro';
import '/public/styles.css';

---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<link rel='stylesheet' href="/public/styles.css" />
		<title>David Mudriy | Interview</title>
	</head>
	<body>
		<header class='top-left'>
			<h1 class="logo">David Mudriy</h1>
			<h3 class='sub-logo'>Interview | <a class='title'>Projects</a></h3>
		</header>
		<section>
			<ProjectLayout 
				title="RSG" 
				subtitleLink="https://www.services.davidmudriy.com/rustscan"
				tags="RustScan, CyberSecurity, GoLang, Port scanning"
				description="Rustscan is a Port Scanner. Itâ€™s a quicker version of nmap. Port Scanning is used for security assessments and networking." />
			<ProjectLayout 
				title="ZNO" 
				subtitleLink="https://www.zinzo.dev" 
				tags="AstroJS, CSS/HTML/JS, Brand Design, Web Design"
				description="Zinzo is a personal Design to Creation-to-Web Demo process test. A simple quick and easy project." />
			<ProjectLayout 
				title="GOR" 
